<div class="container">
  <div class="row">
    <div class="col-8">
      <% @posts.each do |post| %>
      <div class="post-multi">
        <div class="post-meta"><%= post.account.username %>
          <%= image_tag post.image.url, class: "img img-fluid" %>
          <%= post.caption %></div>
          </div>
      <% end %>
    </div>
    <div class="col-4">
      <div class="log-in-meta">
        Logged in as <%= current_account.username %>
      </div>
    </div>
  </div>
</div>